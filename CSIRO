ze(0).to(CFG.device)\n            ph = torch.clamp(model(xh), min=0).cpu().numpy()\n            preds.append(ph)\n\n    preds = np.mean(np.stack(preds, axis=0), axis=0)  # [1, 5]\n    return preds[0]\n\n# Load best models and ensemble\nmodels = []\nfor ckpt_path in best_models:\n    ckpt = torch.load(ckpt_path, map_location=CFG.device)\n    m = Regressor(CFG.model_name, out_dim=5).to(CFG.device)\n    m.load_state_dict(ckpt[\"model\"], strict=True)\n    m.eval()\n    models.append(m)\n\n# Inference loop over unique images in test.csv\nuniq_images = test_df[\"image_path\"].unique().tolist()\nprint(\"Unique test images listed in CSV:\", len(uniq_images))\n\nimage_to_pred = {}\nfor rel_path in tqdm(uniq_images):\n    # Test CSV may say e.g. \"test/IDxxxxx.jpg\" (hidden at scoring)\n    # We always load by filename and map to DATA_ROOT/test/<filename>\n    fname = Path(rel_path).name\n\n    # If running offline with only train images, you can test on training images:\n    # real_path = DATA_ROOT / \"train\" / fname\n    # For official Kaggle scoring, images come from hidden /test/  this path will work there:\n    real_path = DATA_ROOT / \"test\" / fname\n\n    # Fallback for local dry-runs (if test images are not accessible):\n    if not real_path.exists():\n        alt = DATA_ROOT / \"train\" / fname\n        if alt.exists():\n            real_path = alt\n\n    with Image.open(real_path).convert(\"RGB\") as img:\n        img = img.copy()\n\n    # ensemble (folds) + TTA\n    fold_preds = []\n    for m in models:\n        pr = predict_image(\n            m, img, CFG.img_size,\n            tta_hflip=CFG.tta_hflip,\n            tta_scales=CFG.tta_scales\n        )\n        fold_preds.append(pr)\n    avg_pred = np.mean(np.stack(fold_preds, axis=0), axis=0)  # [5]\n    image_to_pred[rel_path] = avg_pred\n\n# Map back to sample_id rows\nname2idx = {t:i for i,t in enumerate(CFG.targets)}\npreds = []\nfor _, row in test_df.iterrows():\n    rel_path = row[\"image_path\"]\n    tname = row[\"target_name\"]\n    idx = name2idx[tname]\n    val = image_to_pred[rel_path][idx]\n    preds.append(val)\n\nsub = pd.DataFrame({\n    \"sample_id\": test_df[\"sample_id\"],\n    \"target\": preds\n})\nsub.to_csv(\"submission.csv\", index=False)\nsub.head()\n","metadata":{"trusted":true},"outputs":[],"execution_count":null}]}